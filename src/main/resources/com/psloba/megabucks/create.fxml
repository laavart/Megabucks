<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.psloba.megabucks.Create">
    <top>
        <HBox alignment="BOTTOM_CENTER" style="-fx-background-color: Gold;" BorderPane.alignment="CENTER">
            <effect>
                <SepiaTone />
            </effect>
            <Label prefHeight="60.0" text="MegaBucks">
                <font>
                    <Font name="System Bold" size="32.0" />
                </font>
                <HBox.margin>
                    <Insets />
                </HBox.margin>
            </Label>
        </HBox>
    </top>
    <bottom>
        <HBox alignment="TOP_CENTER" style="-fx-background-color: Gold;" BorderPane.alignment="CENTER">
            <effect>
                <SepiaTone />
            </effect>
            <Label prefHeight="30.0" text="Label">
                <font>
                    <Font name="System Italic" size="16.0" />
                </font>
            </Label>
        </HBox>
    </bottom>
   <center>
       <VBox alignment="CENTER" BorderPane.alignment="CENTER">
           <GridPane alignment="CENTER" gridLinesVisible="true">
               <columnConstraints>
                   <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                   <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               </columnConstraints>
               <rowConstraints>
                   <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                   <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
               </rowConstraints>
              <VBox.margin>
                  <Insets top="10.0" />
              </VBox.margin>
              <padding>
                  <Insets top="10.0" />
              </padding>
           </GridPane>
           <ButtonBar>
               <buttons>
                   <Button mnemonicParsing="false" onAction="#onBack" text="Back" />
                   <Button mnemonicParsing="false" text="Submit" />
               </buttons>
              <VBox.margin>
                  <Insets />
              </VBox.margin>
              <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
              </padding>
           </ButtonBar>
       </VBox>
   </center>
</BorderPane>
